{
  "name": "space-x-launch-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss '**/*.pcss' --dir src --base src --ext css --verbose",
    "watch:css": "postcss '**/*.pcss' --dir src --base src --ext css --watch --verbose",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "sleep 2 && react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.13.5",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "email-validator": "^2.0.4",
    "enquire.js": "^2.1.6",
    "libphonenumber-js": "^1.7.16",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "mobx-router5": "^4.3.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "primeicons": "^1.0.0",
    "primereact": "^3.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.6.1",
    "react-native-svg-web": "^1.0.1",
    "react-scripts": "2.1.8",

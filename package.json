    "postcss-css-variables": "^0.11.0",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.1%",
    "not ie <= 9",
    "not op_mini all",
    "not dead"

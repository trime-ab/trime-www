@import '../../../variables.pcss';

.home-signup-container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

  &.desktop {
    padding: 0 calc(var(--pageMargin) * 2);
    img {
      min-width: 750px;
    }
  }

  img {
    object-fit: contain;
    max-width: 80%;
  }
}
